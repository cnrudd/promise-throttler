    "version": "0.2.0",
    "configurations": [
        {
      "name": "TESTS: Promise throttle: All",
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "runtimeArgs": ["--experimental-vm-modules"],
      "args": ["--runInBand"],
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js"
    },
    {
      "name": "TESTS: Promise throttle: Respects requestsPerSecond value",
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "runtimeArgs": ["--experimental-vm-modules"],
      "args": [
        "--testNamePattern=respect-the-requestsPerSecond-option"
      ],
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js"
    }
    ]
}